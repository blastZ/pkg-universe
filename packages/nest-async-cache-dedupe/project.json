{
  "name": "nest-async-cache-dedupe",
  "root": "packages/nest-async-cache-dedupe",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tools/scripts/pkgx/index.mjs build nest-async-cache-dedupe"
      }
    },
    "publish": {
      "dependsOn": ["build"],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "packages/nest-async-cache-dedupe/output"
      },
      "configurations": {
        "file": {
          "command": "npm pack"
        },
        "local": {
          "command": "npm link"
        },
        "prod": {
          "command": "npm publish --access public --registry=https://registry.npmjs.org"
        }
      }
    }
  }
}
